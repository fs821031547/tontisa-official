{
  "name": "tontisa-official",
  "version": "1.0.0",
  "description": "同天下官网",
  "private": true,
  "dependencies": {
    "cross-env": "^5.2.0",
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "egg-view-nunjucks": "^2.2.0",
    "less": "^3.8.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "prod": "cross-env NODE_ENV=production EGG_SERVER_ENV=prod egg-scripts start --title=egg-server-tontisa-official --use_strict --port 7003",
    "pre": "cross-env NODE_ENV=production EGG_SERVER_ENV=pre egg-scripts start --title=egg-server-tontisa-official --use_strict --port 7001",
    "dev": "egg-bin dev --use_strict --port 7001",
    "start": "egg-bin dev --use_strict --port 7001 --title=egg-server-tontisa-official",
    "stop": "egg-scripts stop --title=egg-server-tontisa-official",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
